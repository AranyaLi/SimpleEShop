<?
class Orderdetailmodel extends CI_Model{

public function insert($data){
$this->db->insert("OrderDetail", $data);

}

public function get_order(){
	$query=$this->db->get("OrderDetail");
	$data=$query->result_array();
	if($query->num_rows()>0)
	return $data[0];
        else
        return false;

}

}




?>
